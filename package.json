{
  "name": "chat-api",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "scripts": {
    "dev": "moleculer-runner --repl --hot services/**/*.service.js --envfile ./.env.development",
    "start": "moleculer-runner",
    "cli": "moleculer connect NATS",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "lint": "eslint services",
    "dc:up": "docker-compose up --build -d",
    "dc:logs": "docker-compose logs -f",
    "dc:down": "docker-compose down",
    "dc:db": "docker-compose -f docker-compose.yml up database nats redis",
    "generate:migration": "npx sequelize-cli migration:generate --name",
    "generate:seed": "npx sequelize-cli seed:generate --name"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "",
  "devDependencies": {
    "eslint": "^8.25.0",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "moleculer-repl": "^0.7.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "hat": "^0.0.3",
    "ioredis": "^5.0.0",
    "moleculer": "^0.14.26",
    "moleculer-db": "^0.8.20",
    "moleculer-db-adapter-mongo": "^0.4.15",
    "moleculer-db-adapter-sequelize": "^0.2.15",
    "moleculer-io": "^2.2.0",
    "moleculer-web": "^0.10.4",
    "moment": "^2.29.4",
    "mysql2": "^3.6.1",
    "nats": "^2.7.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.33.0",
    "sequelize-cli": "^6.6.1",
    "umzug": "^3.3.1"
  },
  "engines": {
    "node": ">= 16.x.x"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "rootDir": "./services",
    "roots": [
      "../test"
    ]
  }
}
